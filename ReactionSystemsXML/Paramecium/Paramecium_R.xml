<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.sbml.org/sbml/level1 http://www.sbml.org/sbml/level1"
 xmlns="http://www.sbml.org/sbml/level1"
 level="1"
 version="2">

    <model name="Paramecium_R">

		<annotation xmlns:stochSim="AnnotationField">
			<stochSim:TimeStart>			0.0			</stochSim:TimeStart>
			<stochSim:TimeEnd>				10.0		</stochSim:TimeEnd>
			<stochSim:Method>				RLeapingJana </stochSim:Method>
			<stochSim:NumberOfSamples>		1				</stochSim:NumberOfSamples>
			<stochSim:numberOfNoiseLevels>  51			</stochSim:numberOfNoiseLevels>
			<stochSim:Epsilon>				0.03		</stochSim:Epsilon>
			<stochSim:Theta>				0.0			</stochSim:Theta>
			<stochSim:Delta>				0.05		</stochSim:Delta>
			<stochSim:StoreInterval>		25			</stochSim:StoreInterval>
			<stochSim:SortInterval>			10000		</stochSim:SortInterval>
		</annotation>
			
--------------------------------------------------------------------------------------
		<listOfCompartments>
			<compartment volume="1.0" name="comp1" />
			</listOfCompartments>
			
--------------------------------------------------------------------------------------
		<listOfSpecies>
			
			<species compartment="comp1" initialAmount="3000" name="S1"  units="mole">
				</species>
			
			<species compartment="comp1" initialAmount="1500" name="S2"  units="mole">
				</species>
                
		</listOfSpecies>
			
--------------------------------------------------------------------------------------
		
		<listOfReactions>
			<reaction name="R1">
				<listOfReactants>
					<speciesReference species="S1" stoichiometry="2"/>
				</listOfReactants>
				<listOfProducts>
					<speciesReference species="S2" />
				</listOfProducts>
				<kineticLaw formula="">
					<listOfParameters>
						<parameter value="0.000278" name="c1" />
					</listOfParameters>
					</kineticLaw>
				</reaction>
			
			<reaction name="R2">
				<listOfReactants>
					<speciesReference species="S1" stoichiometry="4"/>
					<speciesReference species="S2" stoichiometry="1"/>
				</listOfReactants>
				<kineticLaw formula="">
					<listOfParameters>
						<parameter value="5e-8" name="c2" />
					</listOfParameters>
					</kineticLaw>
				</reaction>
			
			<reaction name="R3">
				<listOfReactants>
                    <speciesReference species="S2" stoichiometry="1"/>
				</listOfReactants>
				<kineticLaw formula="">
					<listOfParameters>
						<parameter value="0.0613" name="c3" />
					</listOfParameters>
					</kineticLaw>
				</reaction>
			
            
			<reaction name="R4">
				<listOfProducts>
					<speciesReference species="S1" />
				</listOfProducts>
				<kineticLaw formula="">
					<listOfParameters>
						<parameter value="127" name="c4" />
					</listOfParameters>
					</kineticLaw>
				</reaction>
                
            
            
			
		</listOfReactions>
		
	</model>	
</sbml>


